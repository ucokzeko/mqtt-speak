{
  "name": "mqtt-speak",
  "version": "0.0.1",
  "description": "Message queue for processing TTS",
  "author": "DSTIL",
  "license": "MIT",
  "repository": {},
  "bugs": {},
  "bin": "./index.js",
  "dependencies": {
    "mqtt": "1.8.0",
    "crypto-js": "3.1.6",
    "request": "2.71.0",
    "config.json": "0.0.4",
    "urijs": "1.16.1"
  },
  "devDependencies": {
    "husky": "0.11.4"
  },
  "engines": {
    "node": ">= 4"
  },
  "keywords": [
    "service",
    "tts",
    "queue"
  ],
  "jshintConfig": {
    "curly": true,
    "devel": true,
    "eqeqeq": true,
    "eqnull": true,
    "evil": true,
    "esversion": 6,
    "expr": true,
    "noarg": true,
    "node": true,
    "onevar": true,
    "strict": true,
    "trailing": true,
    "undef": true,
    "unused": true
  }, 
  "scripts": {
    "precommit": "jshint index.js && cd module/ &&find . -name '*.js' -print0 | xargs -0 jshint"
  }
}