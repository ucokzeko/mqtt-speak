{
  "parser": "babel-eslint",
  "env": {
    "node": true
  },
  "rules": {
    "indent": ["error", 2],
    "comma-dangle": [2, "never"],
    "no-dupe-args": [2],
    "no-extra-semi": [2],
    "eqeqeq": [2],
    "quotes": [2, "single"],
    "eol-last": [0],
    "no-mixed-requires": [0],
    "no-underscore-dangle": [0]
  }
}